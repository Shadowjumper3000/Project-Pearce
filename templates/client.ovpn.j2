client
dev tun
proto udp
remote {{ cloudflare_domain }} {{ openvpn_port }}
resolv-retry infinite
nobind
persist-key
persist-tun
remote-cert-tls server
auth SHA256
cipher AES-256-CBC
comp-lzo
verb 3

<key>
{{ client_private_key }}
</key>

<cert>
{{ client_certificate }}
</cert>

<ca>
{{ ca_certificate }}
</ca>